    <Reference Include="TextEngine" Condition="'$(Platform)' == 'Any CPU'">
      <HintPath>$(MSBuildThisFileDirectory)..\x64\TextEngine.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>